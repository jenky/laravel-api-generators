{
    "name": "jenky/laravel-api-generators",
    "description": "Laravel API generators",
    "keywords": [
        "laravel",
        "api",
        "generators"
    ],
    "homepage": "https://github.com/jenky/laravel-api-generators",
    "license": "MIT",
    "authors": [
        {
            "name": "Linh Tran",
            "email": "jenky.w0w@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "illuminate/console": "~5.1",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\LaravelApiGenerators\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\LaravelApiGenerators\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Jenky\\LaravelGenerators\\GeneratorsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
